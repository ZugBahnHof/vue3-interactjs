{
  "name": "vue3-interactjs",
  "description": "interactjs component for Vue3",
  "version": "1.0.1",
  "keywords": [
    "interact",
    "interactjs",
    "vue",
    "vue3",
    "drag and drop",
    "resize",
    "gesture",
    "vue-cli-plugin-p11n"
  ],
  "license": "MIT",
  "author": "yoroshikudozo",
  "contributors": [
    "yoroshikudozo",
    "ZugBahnHof"
  ],
  "repository": "https://github.com/ZugBahnHof/vue3-interactjs",
  "main": "dist/vue3-interactjs.common.js",
  "module": "dist/vue3-interactjs.esm.js",
  "files": [
    "dist",
    "src",
    "types/index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-interactjs.esm.js",
      "require": "./dist/vue3-interactjs.common.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "yarn build",
    "demo:build": "vite build demo",
    "demo:dev": "vite demo",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "prettier --check . && eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "reformat": "prettier --write . && eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "core-js": "^3.44.0",
    "interactjs": "^1.10.27"
  },
  "peerDependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@interactjs/actions": "^1.10.27",
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vuepress/bundler-vite": "^2.0.0-rc.24",
    "@vuepress/theme-default": "^2.0.0-rc.112",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^10.2.0",
    "prettier": "3.5.3",
    "rollup": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "terser": "^5.14.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.2.0",
    "vuepress": "^2.0.0-rc.24"
  },
  "types": "types/index.d.ts"
}
